<ul>
  <% @tags.each do |tag| %>
    <h5>
      <% tag_freq = tag.contacts.count %>
      <li id="tag_<%= tag.id %>">
        <%= link_to image_tag("iconmonstr-trash-can-15-icon.png", :class => 'trash-can'), tag_path(tag), method: :delete, remote: true %>
        <%= link_to "#{tag.name} (#{tag_freq})", tag_path(tag) %>
      </li>
    </h5>
  <% end %>
</ul>

<%= render 'form', :contact_id => nil %>
